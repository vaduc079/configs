profile tung_monitor {
  output HDMI-A-1 enable mode 3840x2160@60.0Hz position 0,0 transform normal scale 2 adaptive_sync off
  output eDP-1 enable mode 1920x1080@60.001Hz position 1920,0 transform normal scale 1 adaptive_sync off
}

profile default {
  output eDP-1 enable mode 1920x1080@60.001Hz position 0,0
}
